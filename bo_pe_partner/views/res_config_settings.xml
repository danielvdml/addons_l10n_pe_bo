<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.pe.partner</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[last()]" position="after">
                <div class="col-xs-12 col-md-6 o_setting_box" id="partner_autocomplete">
                    <div class="o_setting_left_pane">
                    </div>
                    <div class="o_setting_right_pane" id="partner_autocomplete_settings">
                        <label for="provider_search_partner_by_vat"/>
                        <field name="provider_search_partner_by_vat"/>
                        <div class="text-muted">
                            Selecciona el proveedor para realizar la consulta RUC/DNI
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>